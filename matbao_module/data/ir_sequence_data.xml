<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<record id="sequence_customer" model="ir.sequence">
			<field name="name">Customer</field>
			<field name="code">res.partner</field>
			<field name="prefix">MB</field>
			<field eval="6" name="padding" />
			<field eval="1" name="number_next" />
			<field eval="1" name="number_increment" />
			<field name="company_id" eval="False"/>
		</record>
		
		<record id="sequence_sale_service_number" model="ir.sequence">
			<field name="name">Sale service number</field>
			<field name="code">sale.service.number</field>
			<field name="padding">7</field>
			<field name="company_id" eval="False"/>
		</record>

		<record id="sequence_product" model="ir.sequence">
			<field name="name">Product</field>
			<field name="code">product.product</field>
			<field name="prefix">ERP</field>
			<field eval="5" name="padding" />
			<field eval="1" name="number_next" />
			<field eval="1" name="number_increment" />
			<field eval="False" name="company_id" />
		</record>

	</data>
</odoo>
